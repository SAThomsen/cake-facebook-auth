{
    "name": "sathomsen/cakephp-facebook-authentication",
    "type": "cakephp-plugin",
    "description": "CakePHP plugin for facebook authentication",
    "keywords": [
        "cakephp",
        "authenticate",
        "authentication",
        "facebook",
    ],
    "require": {
        "php": ">=5.5.9",
        "cakephp/cakephp": ">=3.3.2 <4.0.0"
    },
    "require-dev": {
        "phpunit/phpunit": "*"
    },
    "autoload": {
        "psr-4": {
            "FacebookAuth\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "FacebookAuth\\Test\\": "tests",
            "Cake\\Test\\": "./vendor/cakephp/cakephp/tests"
        }
    }
}
